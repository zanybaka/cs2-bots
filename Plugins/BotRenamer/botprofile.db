//
// All profiles begin with this data and overwrite their own
//
Default
	Skill = 100
	Aggression = 95
	ReactionTime = 0.05
	AttackDelay = 0
	Teamwork = 75
	AimFocusInitial = 0.5			// initial focus spread in degrees_[from desired center]
	AimFocusDecay = 0.1				// how much focus shrinks per second_[.25 = 25% of size after 1 sec]
	AimFocusOffsetScale = 0.05		// controls accuracy when tracking to target_[0 == perfect, should always be < 1]
	AimfocusInterval = 0.05			// how often focus is adjusted_[smaller intervals means better movement tracking]
	WeaponPreference = none
	Cost = 0
	Difficulty = EXPERT
	VoicePitch = 100
	Skin = 0
	LookAngleMaxAccelNormal = 2000.0
	LookAngleStiffnessNormal = 100.0
	LookAngleDampingNormal = 25.0
	LookAngleMaxAccelAttacking = 3000.0
	LookAngleStiffnessAttacking = 150.0
	LookAngleDampingAttacking = 30.0
End

